{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/drainAssets.js": {
      "maxDuration": 30
    },
    "api/log.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 30
    },
    "api/test-assets.js": {
      "maxDuration": 30
    },
    "api/preInitialize.js": {
      "maxDuration": 30
    },
    "api/broadcast.js": {
      "maxDuration": 30
    }
  }
}